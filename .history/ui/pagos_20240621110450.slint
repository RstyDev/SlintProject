import { VerticalBox } from "std-widgets.slint";
import { Pago, PagoSt } from "pago.slint";

export component Pagos inherits Rectangle {
    in-out property <[PagoSt]> pagos: [{monto: 30, pagado: 30, id: 30}];
    VerticalBox {
        width: 300px;
        for pay in root.pagos: my-pagos := Pago {
            width: 300px;
            pago: pay;
        }
    }
}
